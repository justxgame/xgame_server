        <mapper resource="mapping/AdvConfigDao.xml"/>
        <mapper resource="mapping/AdvStatisticDao.xml"/>
        <mapper resource="mapping/TokenDao.xml"/>
        <mapper resource="mapping/UserDao.xml"/>-->
