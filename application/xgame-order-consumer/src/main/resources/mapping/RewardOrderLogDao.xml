<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xgame.order.consumer.db.dao.RewardOrderLogDao">




    <select id="getAll" resultType="com.xgame.order.consumer.db.dto.RewardOrderLogDto">
          <![CDATA[
            select server_id,uid,item_id,item_type,item_count,order_type,order_id,is_reorder,indate
          from order_log where order_type=0
        ]]>

    </select>
    <update id="updateObjectById" parameterType="com.xgame.order.consumer.db.dto.RewardOrderLogDto">
        <![CDATA[
            update order_log set order_type=1
             where order_id=${order_id}
        ]]>
    </update>

</mapper>