<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xgame.order.consumer.db.dao.RewardUserDao">





    <select id="getObjectByID" parameterType="java.util.Map" resultType="String">
          <![CDATA[
            SELECT phone_no from user_info where server_id=#{server_id} and uid=#{uid}
        ]]>

    </select>

</mapper>