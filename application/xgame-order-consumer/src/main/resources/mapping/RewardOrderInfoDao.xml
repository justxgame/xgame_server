<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xgame.order.consumer.db.dao.RewardOrderInfoDao">




    <insert id="saveObject" parameterType="com.xgame.order.consumer.db.dto.RewardOrderInfoDto">
           <![CDATA[
            insert into order_info(order_id,req_id,server_id,uid,item_id,item_type,item_count,order_status,order_exception,
            is_reorder,indate)
             values(#{orderId},#{reqId},#{serverId},#{uid},#{itemId},#{itemType},#{itemCount},#{orderStatus},#{orderException},
             #{isReorder},#{indate})
        ]]>
    </insert>
    <insert id="saveObjects" parameterType="java.util.List">
        <![CDATA[
            insert into order_info(order_id,req_id,server_id,uid,item_id,item_type,item_count,order_status,order_exception,
            is_reorder,indate)
             values
        ]]>
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.orderId},#{item.reqId},#{item.serverId},#{item.uid},#{item.itemId},#{item.itemType},#{item.itemCount},#{item.orderStatus},#{item.orderException},
            #{item.isReorder},#{item.indate})
        </foreach>
    </insert>
</mapper>