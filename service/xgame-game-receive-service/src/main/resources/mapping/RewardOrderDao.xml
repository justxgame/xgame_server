<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xgame.service.game.receive.db.dao.RewardOrderDao">



    <insert id="saveObject" parameterType="com.xgame.service.game.receive.db.dto.RewardOrderDto" keyProperty="tansection" useGeneratedKeys="true">
        <![CDATA[
            insert into order_log(server_id,uid,id,item_id,item_type,item_count,order_type,order_id,indate,phone)
             values(#{serverId},#{uid},#{id},#{itemId},#{itemType},#{itemCount},#{orderType},#{orderId},#{indate},#{phone})
        ]]>
    </insert>



</mapper>